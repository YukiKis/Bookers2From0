<%= form_with scope: :search, url: books_search_path, method: :get, local: true do |f| %>
  <%= f.text_field :search %>
  <%= f.select :user_book, options_for_select({ Users: "Users", Books: "Books" }) %>
  <%= f.select :how, options_for_select({ 完全一致: "完全一致", 前方一致: "前方一致", 後方一致: "後方一致", 部分一致: "部分一致" }) %>
  <%= f.submit "SEARCH" %>
<% end %>